{
  "name": "syrnia-tracker",
  "version": "0.5.0",
  "description": "Chrome extension for tracking Syrnia game stats",
  "license": "MIT",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "clean": "rimraf dist",
    "build": "bash bash-scripts/set_global_env.sh && pnpm clean && tsx build/build.ts",
    "dev": "bash bash-scripts/set_global_env.sh CLI_CEB_DEV=true && pnpm clean && tsx build/build.ts",
    "reload": "tsx build/reload.ts",
    "lint": "eslint app/ vite.config.ts",
    "lint:fix": "pnpm lint --fix",
    "format": "prettier --write --cache --cache-location node_modules/.cache/.prettiercache \"app/**/*.{ts,tsx,js,css}\" \"build/**/*.ts\" \"*.{ts,json}\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "type-check": "tsc --noEmit",
    "prepare": "husky",
    "set-global-env": "bash bash-scripts/set_global_env.sh",
    "zip": "tsx build/zip.ts"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^1.44.1",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/react-query": "^5.62.0",
    "@tanstack/react-table": "^8.21.3",
    "clsx": "^2.1.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-error-boundary": "^6.0.0",
    "recharts": "^3.6.0",
    "tailwind-merge": "^3.3.0",
    "type-fest": "^4.41.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.9",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.27.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/chrome": "^0.0.323",
    "@types/eslint-plugin-jsx-a11y": "^6.10.0",
    "@types/node": "^22.15.21",
    "@types/react": "^19.1.5",
    "@types/react-dom": "^19.1.5",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/parser": "^8.32.1",
    "@vitejs/plugin-react-swc": "^4.2.3",
    "autoprefixer": "^10.4.21",
    "deepmerge": "^4.3.1",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "^4.3.5",
    "eslint-plugin-import-x": "^4.12.2",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "fflate": "^0.8.2",
    "globals": "^16.1.0",
    "husky": "^9.1.7",
    "jiti": "^2.6.1",
    "jsdom": "^28.0.0",
    "lint-staged": "^16.0.0",
    "postcss": "^8.5.3",
    "postcss-load-config": "^6.0.1",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "react-spinners": "^0.17.0",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.4.17",
    "tslib": "^2.8.1",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1",
    "vite": "^6.3.5",
    "vite-plugin-node-polyfills": "^0.25.0",
    "vitest": "^4.0.18",
    "ws": "^8.19.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "postcss": {
    "plugins": {
      "tailwindcss": {},
      "autoprefixer": {}
    }
  },
  "packageManager": "pnpm@10.11.0",
  "engines": {
    "node": ">=22.15.1"
  }
}
